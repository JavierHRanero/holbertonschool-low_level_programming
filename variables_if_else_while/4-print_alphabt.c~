#include <stdio.h>
/**
*main - Entry point of the program
*
*Description: This progra assigns a random number to the variable n
*and prints whether the number is positive, negative, or zero.
*
*Return: 0 on success
*/
int main(void)
{
char letra = 'a';
char letra2 = 'A';
while (letra <= 'z')
{
putchar(letra);
letra++;
}
while (letra2 <= 'Z')
{
putchar (letra2);
letra2++;
}
putchar('\n');
return (0);
}
